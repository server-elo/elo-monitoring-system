{
  "analytics": {
    "enabled": true,
    "realtime": true,
    "interval": 5000,
    "metrics": {
      "performance": {
        "track": ["response_time", "throughput", "error_rate"],
        "thresholds": {
          "response_time": 200,
          "error_rate": 0.01
        }
      },
      "usage": {
        "track": ["active_users", "api_calls", "feature_usage"],
        "retention_days": 90
      },
      "development": {
        "track": ["build_time", "test_coverage", "code_quality"],
        "targets": {
          "test_coverage": 80,
          "build_time": 60
        }
      }
    }
  },
  "ai_insights": {
    "enabled": true,
    "models": ["pattern_detection", "anomaly_detection", "prediction"],
    "learning_rate": 0.001,
    "confidence_threshold": 0.85
  },
  "dashboards": {
    "update_frequency": "realtime",
    "widgets": [
      "performance_overview",
      "user_activity",
      "error_tracking",
      "deployment_status",
      "ai_recommendations"
    ]
  },
  "alerts": {
    "channels": ["console", "webhook", "email"],
    "rules": [
      {
        "name": "High Error Rate",
        "condition": "error_rate > 0.05",
        "severity": "critical"
      },
      {
        "name": "Performance Degradation",
        "condition": "response_time > 500",
        "severity": "warning"
      }
    ]
  }
}