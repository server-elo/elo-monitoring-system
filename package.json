{
  "name": "solana-learn-platform",
  "private": true,
  "version": "2.0.0",
  "description": "Next-generation Solidity learning platform with AI-powered features",
  "homepage": "https://github.com/ezekaj/learning_sol",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:clean": "rm -rf .next && npm run dev",
    "ports:clean": "./scripts/clean-ports.sh",
    "ports:analyze": "npx tsx scripts/quantum-port-manager.ts analyze",
    "ports:resolve": "npx tsx scripts/quantum-port-manager.ts resolve --force",
    "build": "prisma generate && next build",
    "build:frontend": "next build",
    "build:analyze": "ANALYZE=true npm run build:frontend",
    "start": "node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist node_modules/.cache",
    "clean:all": "rm -rf .next dist node_modules",
    "clean:build": "rm -rf .next dist",
    "prebuild": "npm run clean:build && npx prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:migrate:ai": "tsx scripts/migrate-enhanced-ai.ts",
    "ai:test": "vitest run tests/enhanced-tutor-system.test.ts",
    "ai:test:integration": "vitest run tests/enhanced-tutor-integration.test.ts",
    "ai:test:all": "vitest run tests/enhanced-tutor*.test.ts",
    "ai:test:runner": "tsx scripts/test-enhanced-tutor.ts",
    "ai:health": "tsx scripts/check-ai-health.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:enhanced-tutor": "vitest run tests/enhanced-tutor*.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "playwright test --grep=\"Performance Testing\"",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config=lighthouserc.mobile.js",
    "performance:analyze": "npm run build:analyze && npm run lighthouse",
    "test:e2e:debug": "playwright test --debug",
    "test:security": "playwright test --grep=\"Security Testing\"",
    "test:collaboration": "playwright test --grep=\"Real-time Collaboration\"",
    "secrets:rotate": "tsx scripts/quantum-secrets-rotation.ts",
    "secrets:rotate:dev": "tsx scripts/quantum-secrets-rotation.ts --environment development --force",
    "secrets:rotate:staging": "tsx scripts/quantum-secrets-rotation.ts --environment staging --force",
    "secrets:rotate:prod": "tsx scripts/quantum-secrets-rotation.ts --environment production --force",
    "secrets:rollback": "tsx scripts/quantum-secrets-rotation.ts --rollback",
    "secrets:schedule": "tsx scripts/quantum-secrets-rotation.ts --schedule",
    "secrets:dry-run": "tsx scripts/quantum-secrets-rotation.ts --dry-run",
    "env:validate": "tsx -e 'import {envValidator} from \"./lib/config/env-validator\"; envValidator.validate(); console.log(\"âœ… Environment validation passed\")'",
    "env:report": "node -e \"const {securityValidator} = require('./lib/security/SecureEnvironmentValidator.ts'); const result = securityValidator.validateEnvironment(); console.log(securityValidator.generateSecurityReport(result));\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:deep-scan": "tsx scripts/quantum-secrets-rotation.ts --environment production --dry-run && npm run env:report",
    "test:ai": "playwright test --grep=\"AI Tutoring\"",
    "test:auth": "echo 'Visit http://localhost:3000/auth/local-test to test authentication'",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:cross-browser": "playwright test --project=\"chromium\" --project=\"firefox\" --project=\"webkit\"",
    "test:ci": "playwright test --reporter=github",
    "test:report": "playwright show-report",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "setup:env": "node scripts/setup-environment.js",
    "setup:env:unix": "node scripts/setup-environment.js",
    "validate:env": "node scripts/validate-environment.js",
    "setup:db": "node scripts/setup-database.js",
    "fix:ts": "node scripts/fix-typescript-errors.js",
    "fix:syntax": "node scripts/quick-syntax-fix.js",
    "fix:syntax:deep": "tsx scripts/fix-all-syntax-errors.ts",
    "fix:syntax:targeted": "node scripts/targeted-syntax-fix.js",
    "test:db": "node scripts/test-database.js",
    "deployment:checklist": "node scripts/deployment-checklist.js",
    "postinstall": "npm run playwright:install",
    "docker:build": "docker build -t solana-learn .",
    "docker:run": "docker run -p 3000:3000 solana-learn",
    "legacy:dev": "vite",
    "legacy:build": "vite build",
    "legacy:preview": "vite preview",
    "socket:dev": "cd socket-server && npm run dev",
    "socket:start": "cd socket-server && npm start",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "railway up",
    "deploy:preview": "vercel",
    "postbuild": "next-sitemap",
    "prp:create": "tsx scripts/prp-create.ts",
    "prp:execute": "tsx scripts/prp-execute.ts",
    "prp:validate": "tsx scripts/prp-validate.ts",
    "prp:list": "ls -la PRPs/*.md | grep -v README",
    "prp-master": "tsx scripts/prp-master-quantum.ts",
    "prp-master:analyze": "tsx scripts/prp-master-quantum.ts quantum analyze",
    "prp-master:predict": "tsx scripts/prp-master-quantum.ts quantum predict",
    "prp-master:debug": "tsx scripts/prp-master-quantum.ts quantum debug",
    "prp-master:evolve": "tsx scripts/prp-master-quantum.ts quantum evolve",
    "prp-master:status": "tsx scripts/prp-master-quantum.ts quantum status",
    "prp-master:help": "tsx scripts/prp-master-quantum.ts help",
    "prp-master:demo": "tsx scripts/prp-master-quantum.ts demo",
    "prp-master:init": "tsx scripts/prp-master-quantum.ts init",
    "prp:test-enhanced": "tsx lib/prp/test-enhanced-prp-master.ts",
    "db:deploy": "prisma migrate deploy",
    "db:seed:prod": "NODE_ENV=production npm run db:seed",
    "dev:debug": "NODE_OPTIONS='--inspect' npm run dev",
    "test:components": "echo 'Visit http://localhost:3000/auth/demo to see component showcase'",
    "check:all": "npm run type-check && npm run lint",
    "fix:all": "npm run lint -- --fix && npm run format",
    "bundle:analyze": "tsx scripts/bundle-analysis.ts",
    "bundle:stats": "ANALYZE=true npm run build && npm run bundle:analyze",
    "bundle:budget": "tsx scripts/check-budget.ts",
    "bundle:report": "tsx scripts/generate-bundle-report.ts",
    "export-bundle-stats": "webpack-bundle-analyzer .next/analyze/bundle-stats.json .next/analyze --mode=json --report=.next/webpack-stats.json",
    "analyze-bundle": "npm run bundle:analyze && npm run bundle:report",
    "web-vitals:test": "tsx scripts/test-web-vitals.ts",
    "web-vitals:report": "tsx scripts/web-vitals-report.ts",
    "test:web-vitals": "npm run web-vitals:test && npm run web-vitals:report",
    "monitoring:dashboard": "tsx scripts/generate-monitoring-dashboard.ts",
    "monitoring:export": "tsx scripts/export-monitoring-data.ts",
    "generate-monitoring-dashboard": "npm run monitoring:dashboard && npm run monitoring:export",
    "performance:regression": "tsx scripts/check-performance-regression.ts",
    "check-performance-regression": "npm run performance:regression",
    "security:check": "npm run security:audit && depcheck",
    "lighthouse:ci": "lhci autorun --config=lighthouse.config.js",
    "lighthouse:local": "lighthouse http://localhost:3000 --output=html --output-path=.lighthouse/report.html",
    "performance:test": "npm run lighthouse:ci && npm run test:web-vitals && npm run bundle:budget",
    "start:production": "NODE_ENV=production node server.js",
    "worker:start": "node workers/index.js",
    "healthcheck": "node scripts/healthcheck.js",
    "12factor:check": "node scripts/12factor-check.js",
    "12factor:quick": "node scripts/12factor-check.js",
    "prp:scaffold": "node scripts/12factor-check.js && node scripts/scaffold-missing.js",
    "cluster:status": "tsx lib/workers/worker-manager.ts",
    "workers:start": "npm run worker:start",
    "workers:stop": "pkill -f 'workers/index.js'",
    "env:example": "tsx -e 'import {envValidator} from \"./lib/config/env-validator\"; console.log(envValidator.generateExample())' > .env.example",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "build:cluster": "npm run build && npm run env:validate",
    "quantum:optimize": "tsx scripts/quantum-performance-validator.ts",
    "quantum:validate": "npm run build && npm run quantum:optimize",
    "quantum:benchmark": "npm run quantum:validate && npm run lighthouse:ci",
    "quantum:full-deploy": "npm run quantum:benchmark && npm run deploy:vercel",
    "quantum:check": "tsx scripts/validate-optimizations.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
