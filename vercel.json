{
  "version": 2,
  "builds": [
    {
      "src": "central-monitoring-pro.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/central-monitoring-pro.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "central-monitoring-pro.js": {
      "maxDuration": 30
    }
  }
}