# Essential environment variables for production build
NODE_ENV=production

# Required for NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=development-secret-change-in-production-123456

# Database (using SQLite for now)
DATABASE_URL=file:./dev.db

# Basic app config
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Solidity Learning Platform

# Disable features that require external services
FEATURE_AI_TUTORING=false
FEATURE_COLLABORATION=false
FEATURE_BLOCKCHAIN_INTEGRATION=false

# Mock Redis URL (not used in production build)

# AgentOps Monitoring
AGENTOPS_API_KEY=your_agentops_api_key
AGENTOPS_ENV=production
AGENTOPS_TAGS=solidity-learning,v1.0
REDIS_URL=redis://localhost:6379